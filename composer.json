{
    "name": "badass-dd/lazy-docs",
    "description": "Automatically generate intelligent, Scribe-compatible PHPDoc for Laravel API controllers using AST parsing",
    "type": "library",
    "license": "MIT",
    "version": "dev-main",
    "autoload": {
        "psr-4": {
            "Badass\\LazyDocs\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Badass\\LazyDocs\\LazyDocsServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "2.0.x-dev"
        }
    },
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "nikic/php-parser": "^5.0",
        "fakerphp/faker": "^1.23",
        "doctrine/dbal": "^3.0",
        "symfony/var-exporter": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}