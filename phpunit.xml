{
    "name": "badass-dd/lazy-docs",
    "description": "Automatically generate intelligent, Scribe-compatible PHPDoc for Laravel API controllers using AST parsing",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Badass\\LazyDocs\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "nikic/php-parser": "^5.0",
        "fakerphp/faker": "^1.23",
        "doctrine/dbal": "^3.0",
        "symfony/var-exporter": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Badass\\LazyDocs\\LazyDocsServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}